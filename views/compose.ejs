<%- include("partial/header.ejs") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group  ">
    <label for="postTitle" name="postTitle">Title</label>
    <input type="text" name="postTitle" cutocomplete="off" class="form-control">
    
    <label for="postBody" name="postBody">Post</label>
    <textarea type="text" name="postBody" cutocomplete="off" rows="8" class="form-control" > </textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partial/footer.ejs") -%>
